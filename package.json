{
	"name": "redcode",
	"displayName": "Redcode",
	"description": "Syntax highlighting and snippets for Corewar's Redcode",
	"version": "1.0.0",
	"publisher": "David Gozlan",
	"engines": {
	  "vscode": "^1.70.0"
	},
	"categories": ["Programming Languages"],
	"contributes": {
	  "languages": [
		{
		  "id": "redcode",
		  "aliases": ["Redcode"],
		  "extensions": [".s"],
		  "configuration": "./language-configuration.json"
		}
	  ],
	  "grammars": [
		{
		  "language": "redcode",
		  "scopeName": "source.s",
		  "path": "./syntaxes/redcode.json"
		}
	  ],
	  "snippets": [
		{
		  "language": "redcode",
		  "path": "./snippets/redcode.json"
		}
	  ]
	}
  }
  