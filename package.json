{
	"name": "corewar-code",
	"displayName": "Corewar Redcode [Epitech]",
	"description": "Syntax highlighting and snippets for Corewar's Redcode",
	"version": "1.0.0",
	"publisher": "davphla",
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"Corewar",
		"Champions"
	],
	"main": "./src/main.js",
	"capabilities": {
		"hoverProvider": "true"
	},
	"activationEvents": [
		"onLanguage:redcode"
	],
	"contributes": {
		"languages": [
			{
				"id": "redcode",
				"aliases": [
					"Redcode"
				],
				"extensions": [
					".s"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "redcode",
				"scopeName": "source.s",
				"path": "./syntaxes/redcode.json"
			}
		],
		"snippets": [
			{
				"language": "redcode",
				"path": "./snippets/redcode.json"
			}
		]
	}
}